{
  "name": "normandpllc-site",
  "version": "1.0.0",
  "description": "Normand PLLC Law Firm Website",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "dev:vercel": "vercel dev --listen 7002",
    "start": "node server.js",
    "deploy": "vercel --prod",
    "menu:watch": "node scripts/menu-watcher.js",
    "menu:check": "node scripts/menu-watcher.js check",
    "menu:sync": "node scripts/menu-watcher.js sync",
